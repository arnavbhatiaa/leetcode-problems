class Solution {
    public int[] smallerNumbersThanCurrent(int[] nums) {
        int count =0;
        int length= nums.length;
        int[] array= new int [length];
        for (int i=0;i<length;i++){
            count=0;
            for (int j=0;j<length;j++){
                if(nums[i]>nums[j]){
                    count++;
                }
            }
            array[i]=count;}
            return array;}}
