class Solution {
    public int[] twoSum(int[] nums, int target) {
      int length= nums.length;
      int one=0;
      int two=0;
        for (int i=0;i<length;i++){
            for (int j=0;j<length;j++){
                if (i!=j){
                    if (nums[i]+nums[j]==target){
                        one=i;
                        two=j;
                       
                    }
                }

            }
    }
    int[] arr= {two,one};
    return arr;
}}
