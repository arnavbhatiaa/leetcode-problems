class Solution {
    public boolean isPalindrome(int x) {
        String X = String.valueOf(x);
        int length= X.length();
     int right= length-1;
        for (int i=0;i<length;i++){
if (X.charAt(i)!=X.charAt(right)){
return false;
        }
        else{right--;}}
        return true;
    
}}
